 Help
Š--/-/-------------------------------------------------------------------------
Z         Z/ /                                           ColumnsZ    Z    
Command ===>Z/ /                                            Scroll ===>ÜZ   
ZDATA,ZSHADOW/ /                                                              
/ /                                                                           
)INIT
.ZVARS = '(ZVMODET ZTITLE ZCL ZCR ZCMD ZSCED)'
&ZHIDEX = 'Y'
.HELP = ZIGIH272
&zpm3 = 0
VGET (ZSCED) PROFILE        /* Fill Scroll Vars if       */
IF (&ZSCED = ' ') &ZSCED = 'PAGE'  /* Blank with page    */
&MIXED = TRANS(&ZPDMIX N,EBCDIC *,MIX) /* set mixed format */
*REXX(* zdata zshadow zwidth)
  color = left('W',50,'W')
  blank = left(' ',zwidth,' ')
  if datatype(zhead) /= 'NUM' then
     parse value '0 0 0 ' with zhead zmiddle ztrail
  if length(zshadow) /= length(zdata) then
     zshadow = left(' ',length(zdata),' ')
  do i = 1 to length(zshadow) by zwidth
     Select
     when substr(zdata,i+8,7) = '>>>>>>>' then do
         zshadow = overlay(color,zshadow,i+1,6)
         len = substr(zdata,i+8,zwidth-8)
         len = translate(len,' ','00'x)
         len = length(strip(len))
         zshadow = overlay(color,zshadow,i+8,len)
         end
     when substr(zdata,i+8,7) = '<<<<<<<' then do
         zshadow = overlay('WWWWWW',zshadow,i+1,6)
         len = substr(zdata,i+8,zwidth-8)
         len = translate(len,' ','00'x)
         len = length(strip(len))
         zshadow = overlay(color,zshadow,i+8,len)
         end
     when substr(zdata,i+8,7) = '=======' then do
         zshadow = overlay('WWWWWW',zshadow,i+1,6)
         len = substr(zdata,i+8,zwidth-8)
         len = translate(len,' ','00'x)
         len = length(strip(len))
         zshadow = overlay(color,zshadow,i+8,len)
         end
     otherwise do
         len = substr(zdata,i+8,zwidth-8)
         len = translate(len,' ','00'x)
         len = length(strip(len))
         zshadow = overlay(blank,zshadow,i+8,len)
         end
     end
     end
*ENDREXX
)REINIT
REFRESH(*)
.HELP = ZIGIH272
)PROC
REFRESH(*)
&ZCURSOR = .CURSOR
&ZCSROFF = .CSRPOS
VPUT (ZSCED) PROFILE
&ZLVLINE = LVLINE(ZDATA)
)FIELD
FIELD(ZTITLE)
FIELD(ZCMD) LEN(255)
)END
